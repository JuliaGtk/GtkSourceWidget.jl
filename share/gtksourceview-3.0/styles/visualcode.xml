<?xml version="1.0" ?>
<style-scheme id="visualcode" name="Autumn" version="1.0">
	<author/>
	<_description>Autumn theme</_description>
	<style name="text" 							background="#FDF6E3" 		foreground="#3f4444" />
	<style name="cursor" 						background="#red" 			foreground="#black" />
	<style name="diff:added-line" 	background="#447744" 		foreground="#aaeeaa" />
	<style name="diff:changed-line" background="#ddbb55" 		foreground="#lightyellow" />
	<style name="diff:removed-line" background="#aaccaa" 		foreground="#336633" />
	<style name="line-numbers" 			background="#lightgrey" foreground="#brown" />
	<style name="search-match" 			background="#yellow" 		foreground="#black" />
	<style name="def:comment" 			background="#FDF6E3" 				foreground="#93A1A1" />
	<style name="def:constant" 			background="#FDF6E3" 				foreground="#bb6666" />
	<style name="def:specials" 			background="#FDF6E3" 				foreground="#darkcyan" 	bold="true"  />
	<style name="def:identifier" 		background="#FDF6E3" 				foreground="#darkcyan" 	bold="true"  />
	<style name="def:statement" 		background="#FDF6E3" 				foreground="#darkcyan"  />
	<style name="def:preprocessor" 		background="#FDF6E3" 				foreground="#darkgrey" />
	<style name="def:type" 				background="#FDF6E3" 				foreground="#bb9900" 		bold="true"  />
	<style name="def:function" 			background="#FDF6E3" 				foreground="#268BD2" 		 />
  <style name="selection"         		background="#gray" 				foreground="#black" />
  <style name="def:note" 				background="#FDF6E3" 				foreground="#2758a5" 			bold="true" />
    <style name="def:keyword" 			background="#FDF6E3" 				foreground="#859900" />
  
</style-scheme>



